{
  "name": "trpc-router",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/trpc-router/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/trpc-router",
        "main": "packages/trpc-router/src/index.ts",
        "tsConfig": "packages/trpc-router/tsconfig.lib.json",
        "assets": ["packages/trpc-router/*.md"]
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx nx run trpc-router:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/trpc-router/**/*.ts",
          "packages/trpc-router/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/trpc-router/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
